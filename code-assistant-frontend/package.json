{
  "name": "code-assistant-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/register": "^7.24.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "css-loader": "^7.1.2",
    "express": "^4.19.2",
    "html-react-parser": "^5.1.16",
    "ignore-styles": "^5.0.1",
    "jodit": "^4.2.27",
    "jodit-react": "^4.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.24.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ssr": "npm run build && node server/index.js",
    "ssrdev": "npm-run-all --parallel -p build  && nodemon ./build/index.js && node server/index.js  webpack --config webpack.server.js --mode=development -w ",
    "dev:build-server": "NODE_ENV=development webpack --config webpack.server.js --mode=development -w",
    "dev:start": "nodemon ./build/index.js",
    "dev": "npm-run-all --parallel build dev:*",
    "build:client": "webpack --config webpack/client.config.js",
    "build:server": "webpack --config webpack/server.config.js",
    "build": "npm run build:client && npm run build:server",
    "start:dev": "concurrently \"npm run build:server && nodemon --exec babel-node src/server/index.js\"  \"npm run build:client -- --watch\"",
    "start": "npm run start:dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.25.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
